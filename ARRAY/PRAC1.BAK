#include<conio.h>
#include<stdio.h>
int sortc(int a[],int sz)
{
	int t;
	for(int i=0;i<sz;i++)
	{
		if(a[i+1]>a[i])
		t=1;
		else
		return (-1);
	}
	return t;
}
